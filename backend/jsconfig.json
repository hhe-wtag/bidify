    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2022",
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "service-worker.js"],
  "exclude": ["node_modules", "dist"]
}
